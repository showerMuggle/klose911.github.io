;; cons cell  ;;
;;;;;;;;;;;;;;;;

;;; read 
'(1 . 2)                                ; => (1 . 2)
'(?a . 1)                               ; => (97 . 1)
'(1 . "a")                              ; => (1 . "a")
'(1 . nil)                              ; => (1)
'(nil . nil)                            ; => (nil)

(read "(1 . 2)")                        ; => (1 . 2)

;;; empty list 
nil                                     ; => nil
'()                                     ; => nil

(car nil)                               ; => nil
(cdr nil)                               ; => nil

;;; type
'(1 2 3)                                  ; => (1 2 3)
'(1 2 . 3)                                ; => (1 2 . 3)
'(1 . #1=(2 3 . #1#))                     ; => (1 2 3 . #1)


'(1 . (2 . (3 . nil)))                  ; => (1 2 3)
